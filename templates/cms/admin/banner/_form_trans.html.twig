{% import 'adminTemplate/macros.html.twig' as macros %}
{% set trans_form = form.translations[locale] %}
<div class="panel panel-flat">
    <div class="panel-body">
        <div class="row">
            {{ macros.input(trans_form.title) }}
            {{ macros.input(trans_form.text) }}
        </div>
        <div class="row">
            {{ macros.input(trans_form.url,6,'Hint : https://www.google.com') }}
            {{ macros.input(trans_form.actionButton) }}
        </div>

    </div>
    <div class="panel-footer">
        {% include 'adminTemplate/saveAndResetFormFooter.html.twig' %}
    </div>
</div>
