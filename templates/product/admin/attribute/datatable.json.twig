{
    "recordsTotal": {{ recordsTotal }},
    "recordsFiltered": {{ recordsFiltered }},
    "data": [
        {% for attribute in attributes %}
        [
            "{{ attribute.id }}",
            "{{ attribute.title|pn_json_encode }}",
            "{% if attribute.tarteb %}{{ attribute.tarteb }}{% else %}N/A{% endif %}",
            "{{ attribute.typeName }}",
            "{% if attribute.search == true %}<span class='label label-success'>Yes</span>{% else %}<span class='label label-danger'>No</span>{% endif %}",
            "{% if attribute.mandatory == true %}<span class='label label-success'>Yes</span>{% else %}<span class='label label-danger'>No</span>{% endif %}",
            "<ul class='icons-list'><li class='dropdown'><a href='#' class='dropdown-toggle' data-toggle='dropdown'><i class='icon-menu9'></i></a><ul class='dropdown-menu dropdown-menu-right'><li><li><a href='{{ path('attribute_edit',{'id':attribute.id}) }}'><i class='icon-pencil7'></i> Edit</a></li><li><a href='#' class='delete-btn' data-toggle='modal' data-target='#modal_delete' data-delete='{{ path('attribute_delete',{'id':attribute.id}) }}'><i class='icon-bin'></i> Remove</a></li></ul></li></ul>"

        ]{% if loop.last == false %},{% endif %}
        {% endfor %}
    ]
}