        {% if product.details.augmentedRealityUrl is not empty %}
            <br>
            <a class="l-product-cart-bottom-action ui-product-cart-bottom-action"
               href="{{ product.details.augmentedRealityUrl }}"
               rel="noreferrer"
               target="_blank"
            >
                Augmented Reality Preview
            </a>
        {% endif %}
    </div>
</div>
