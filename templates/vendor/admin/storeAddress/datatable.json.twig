{
    "recordsTotal": {{ recordsTotal }},
    "recordsFiltered": {{ recordsFiltered }},
    "data": [
        {% for storeAddress in storeAddresses %}
        [
            "{{ storeAddress.id }}",
            "{{ storeAddress.fullAddress }}",
            "{% if storeAddress.zone %}{{ storeAddress.zone.title }}{% else %}N/A{% endif %}",
            "{% if storeAddress.created %}{{ storeAddress.created|date('d/m/Y') }}{% else %}--{% endif %}",
            "<ul class='icons-list'><li class='dropdown'><a href='#' class='dropdown-toggle' data-toggle='dropdown'><i class='icon-menu9'></i></a><ul class='dropdown-menu dropdown-menu-right'>{% if is_granted('ROLE_ADMIN') %}<li><a href='{{ path('store_address_edit',{'id':storeAddress.id}) }}'><i class='icon-pencil7'></i> Edit</a></li><li><a href='#' class='delete-btn' data-toggle='modal' data-target='#modal_delete' data-delete='{{ path('store_address_delete',{'id':storeAddress.id}) }}'><i class='icon-bin'></i> Remove</a></li>{% endif %}</ul></li></ul>"

        ]{% if loop.last == false %},{% endif %}
        {% endfor %}
    ]
}