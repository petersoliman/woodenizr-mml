{
    "recordsTotal": {{ recordsTotal }},
    "recordsFiltered": {{ recordsFiltered }},
    "data": [
        {% for vendor in vendors %}
        [
            "{{ vendor.id }}",
            "{{ vendor.title }}",
            "{{ vendor.commissionPercentage }}%",
            "{% if vendor.created %}{{ vendor.created|date('d/m/Y') }}{% else %}--{% endif %}",
            "<ul class='icons-list'><li class='dropdown'><a href='#' class='dropdown-toggle' data-toggle='dropdown'><i class='icon-menu9'></i></a><ul class='dropdown-menu dropdown-menu-right'><li><a href='{{ path('store_address_index', {"id":vendor.id }) }}'><i class='icon-map'></i> Store Address</a></li><li class='divider'></li>{% if is_granted('ROLE_ADMIN') %}<li><a href='{{ path('vendor_edit',{'id':vendor.id}) }}'><i class='icon-pencil7'></i> Edit</a></li><li><a href='#' class='delete-btn' data-toggle='modal' data-target='#modal_delete' data-delete='{{ path('vendor_delete',{'id':vendor.id}) }}'><i class='icon-bin'></i> Remove</a></li>{% endif %}</ul></li></ul>"

        ]{% if loop.last == false %},{% endif %}
        {% endfor %}
    ]
}