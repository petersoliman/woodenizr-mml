{
    "recordsTotal": {{ recordsTotal }},
    "recordsFiltered": {{ recordsFiltered }},
    "data": [
        {% for paymentMethod in paymentMethods %}
        [
            "<a href='{{ path('payment_method_edit',{'id':paymentMethod.id}) }}'>{{ paymentMethod.title }}</a><br><small>({{ paymentMethod.typeName }})</small>",
            "{% if paymentMethod.fees %}{{ paymentMethod.fees }} EGP{% else %}N/A{% endif %}",
            "{% if paymentMethod.active %}<span class='label label-success'>Yes</span>{% else %}<span class='label label-danger'>No</span>{% endif %}",
            "{% if paymentMethod.modified %}{{ paymentMethod.modified|date('d/m/Y') }}{% else %}--{% endif %}",
            "<ul class='icons-list'><li class='dropdown'><a href='#' class='dropdown-toggle' data-toggle='dropdown'><i class='icon-menu9'></i></a><ul class='dropdown-menu dropdown-menu-right'><li><a href='{{ path('payment_method_edit',{'id':paymentMethod.id}) }}'><i class='icon-pencil7'></i> Edit</a></li></ul></li></ul>"
        ]{% if loop.last == false %},{% endif %}
        {% endfor %}
    ]
}